pipeline{

	agent {label 'Build_server'}
          stages {
	    
	    stage('gitclone') {

			steps {
				checkout scm
			}
		}
              stage('Build_package') {

			steps {
				sh 'mvn clean package'
			}
		}
		stage('Build') {

			steps {
				sh 'mvn clean package.'
			}
		}

		stage('Login') {

			steps {
				sh 'docker login -u akshayarvinds -p Boss@8147'
			}
		}

		stage('Push') {

			steps {
				sh 'docker push akshayarvinds/newdockerimage:latest'
			}
		}
	}

	post {
		always {
			sh 'docker logout'
		}
	}

}
